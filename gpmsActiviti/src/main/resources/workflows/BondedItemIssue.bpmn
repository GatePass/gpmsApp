    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="bondedItemCreatedMailtask"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="bondedItemCreatedMailtask" targetRef="ISITMgrApprovalTask"></sequenceFlow>
