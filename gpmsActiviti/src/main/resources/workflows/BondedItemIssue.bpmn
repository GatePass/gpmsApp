    </sequenceFlow>
    <serviceTask id="RejectMailToISITUser" name="Reject Mail to ISIT User" activiti:class="org.gpms.web.TestActiviti"></serviceTask>
    <sequenceFlow id="Rejected" name="Rejected" sourceRef="exclusivegateway1" targetRef="RejectMailToISITUser">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${exclusiveGateway.getDecision() == false}]]></conditionExpression>
